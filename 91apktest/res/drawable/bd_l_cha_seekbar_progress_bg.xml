<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#49ffffff" android:endColor="#49ffffff" android:angle="270.0" android:centerY="0.75" android:centerColor="#49ffffff" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffff0000" android:endColor="#ff00ff00" android:angle="90.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#fff8c068" android:endColor="#ccf8c068" android:angle="180.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>